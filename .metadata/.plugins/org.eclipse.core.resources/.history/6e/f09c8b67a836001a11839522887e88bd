package com.bridgelabz.functionalprgm;

import java.util.Random;

public class CouponNumber {

	public static void main(String[] args) {
		Random gen = new Random(); 
		//int randomNum= gen.nextInt(100);
		int[] myarray = null;
	for(int i=0;i<=7;i++)
	{
		int randomNum= gen.nextInt(100);
		myarray[i] = randomNum;
		

	}
	
	
System.out.println(myarray);
	}
}

	

